	<!-- Google Fonts -->
	<link href="https://fonts.googleapis.com/css?family=Raleway:400,600&display=swap" rel="stylesheet">
	
	<!-- CSS style -->
	<style type="text/css">
		body {
			margin: 0;
			padding: 0;
			font-family: 'Raleway', sans-serif;
			font-size: 17px;
		}

		a {
			text-decoration: none;
			cursor: pointer;
			color: inherit;
		}

		.nav {
			width: 100%;
			height: 70px;
			background-color: #f5f5f5;
		}
		.nav-inner {
			width: 60%;
			margin: auto;
			font-size: 19px;
			line-height: 70px;
       		white-space: nowrap;
		}
		.nav-inner .left, .nav-inner .right {
			width: 50%;
			display: inline-block;
			vertical-align: top;
		}
		.nav-inner .right {
			display: inline-flex;
			flex-flow: row-reverse;
		}
		.nav-inner .left .logo {
			font-size: 27px;
			color: black;
		}
		.nav-inner .left .logo span {
			font-weight: 600;
		}
		.nav-inner .right .link {
			display: inline-block;
			margin-left: 20px;
			margin-right: 20px;
			color: #7b7b7b;
			position: relative;
		}
		.nav-inner .right .link:hover {
			color: black;
		}
		.nav-inner .right .active.link:before {
			content: "<";
			position: absolute;
			left: -15px;
			font-size: 25px;
			top: -1px;
		}
		.nav-inner .right .active.link:after {
			content: ">";
			position: absolute;
			right: -15px;
			font-size: 25px;
			top: -1px;
		}

		.content {
			width: 60%;
			margin: auto;
		}

		.main {
			width: 70%;
			margin: auto;
			margin-top: 100px;
		}
	</style>
</head>
<body>
